{
  "name": "unocss-preset-prime",
  "type": "module",
  "version": "6.0.0",
  "description": "UnoCSS preset for supporting Prime UI theme colors and icons",
  "author": "Daniel Waltz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielwaltz/unocss-preset-prime.git"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "prime"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./theme": {
      "types": "./dist/theme.d.mts",
      "default": "./dist/theme.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "obuild",
    "stub": "obuild --stub",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --max-warnings 0",
    "type-check": "tsc --noEmit",
    "publint": "npx -y publint@latest && npx -y @arethetypeswrong/cli@latest --pack --profile esm-only",
    "test": "run-s build lint type-check publint",
    "playground:dev": "vite --config ./playground/vite.config.ts",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "peerDependencies": {
    "@iconify-json/prime": ">= 1.1.5",
    "unocss": ">= 66.0.0 < 100"
  },
  "peerDependenciesMeta": {
    "@iconify-json/prime": {
      "optional": true
    }
  },
  "devDependencies": {
    "@danielwaltz/eslint-config": "^3.0.1",
    "@iconify-json/prime": "^1.2.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "~24.10.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@unocss/eslint-plugin": "^66.6.0",
    "@unocss/preset-mini": "^66.6.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "npm-run-all2": "^8.0.4",
    "obuild": "^0.4.14",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "semantic-release": "^25.0.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "vite": "^7.3.1"
  },
  "overrides": {
    "conventional-changelog-conventionalcommits": ">=9.1.0"
  }
}
