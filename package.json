{
  "name": "unocss-preset-prime",
  "version": "4.1.0",
  "description": "UnoCSS preset for supporting Prime UI theme colors and icons",
  "type": "module",
  "keywords": [
    "unocss",
    "unocss-preset",
    "prime"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielwaltz/unocss-preset-prime.git"
  },
  "author": "Daniel Waltz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./theme": {
      "types": "./dist/theme.d.mts",
      "default": "./dist/theme.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --max-warnings 0",
    "type-check": "tsc --noEmit",
    "publint": "npx -y publint@latest && npx -y @arethetypeswrong/cli@latest --pack --profile esm-only",
    "test": "npm run lint && npm run type-check && npm run build && npm run publint",
    "playground:dev": "vite --config ./playground/vite.config.ts",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "peerDependencies": {
    "@iconify-json/prime": ">= 1.1.5",
    "unocss": ">= 66.0.0 < 100"
  },
  "peerDependenciesMeta": {
    "@iconify-json/prime": {
      "optional": true
    }
  },
  "devDependencies": {
    "@danielwaltz/eslint-config": "^2.2.1",
    "@iconify-json/prime": "^1.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "~22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@unocss/eslint-plugin": "^66.0.0",
    "@unocss/preset-mini": "^66.0.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^9.24.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "semantic-release": "^24.2.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "unocss": "^66.0.0",
    "vite": "^6.2.6"
  },
  "overrides": {
    "conventional-changelog-conventionalcommits": ">= 8.0.0"
  }
}
