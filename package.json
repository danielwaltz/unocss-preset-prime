{
  "name": "unocss-preset-prime",
  "version": "3.2.2",
  "description": "UnoCSS preset for supporting Prime UI theme colors and icons",
  "license": "MIT",
  "author": "Daniel Waltz",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielwaltz/unocss-preset-prime.git"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "prime"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "chore",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --no-fix --ignore-path .gitignore --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "playground:dev": "vite --config ./playground/vite.config.ts",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "peerDependencies": {
    "@iconify-json/prime": "*",
    "unocss": ">= 0.45.0 < 1"
  },
  "peerDependenciesMeta": {
    "@iconify-json/prime": {
      "optional": true
    }
  },
  "devDependencies": {
    "@iconify-json/prime": "^1.1.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "~18.17.19",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^22.0.5",
    "standard-version": "^9.5.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "vite": "^4.5.0"
  }
}
